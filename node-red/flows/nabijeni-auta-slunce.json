    {
        "id": "d027326abd49e85d",
        "type": "function",
        "z": "49921d17efb0cefe",
        "name": "Baterky jsou > min_soc_slunce?",
        "outputs": 2,
        "x": 340,
        "y": 220,
        "wires": [
            [
                "04485a466ad52c1e"
            ],
            [
                "5ae66c11fe946b89"
            ]
        ],
        "func": "// Zkontroluje zda SOC baterie > min SOC pro solární nabíjení (z fve_config)\n// Výstup 1: SOC > min_soc_slunce → ok, pokračuj\n// Výstup 2: SOC <= min_soc_slunce → baterie potřebuje nabít, zastav\nconst config = global.get(\"fve_config\") || {};\nconst minSoc = config.nabijeni_auta_min_soc_slunce || 85;\nconst stateObj = global.get(\"homeassistant.homeAssistant.states['sensor.battery_percent']\") || {};\nconst soc = parseFloat(stateObj.state || \"0\");\nmsg.payload = soc;\nnode.status({fill: soc > minSoc ? \"green\" : \"orange\", shape: \"dot\", text: soc + \"% (min \" + minSoc + \"%)\"});\nif (soc > minSoc) return [msg, null];\nreturn [null, msg];\n",
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "g": "nabijeni_slunce_grp_vstup"
    },
